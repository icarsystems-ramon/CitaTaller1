<h2 id="title">Bienvenido a taller</h2>
{{paper-input label="Nombre" value=model.nombre icon="person" icon-class="name"}}
{{paper-input label="Apellidos" value=model.apellidos icon="person" icon-class="name" required=true required-errortext="Se requiere apellidos"}}
{{paper-input label="Teléfono" value=model.numgsm icon="phone" md-no-float=''}}
{{paper-input label="Email" value=model.email icon="email" icon-class="email"}}
{{paper-input label='Matrícula' value=model.matric icon="maps" icon-class="directions-car"}}

{{paper-divider}}

{{#if multiTaller}}
    {{select-taller model=listTaller chooseTaller="chooseTaller"}}
{{/if}}

{{#if vehiculoCortesiaEnabled}}
    {{#paper-switch checked=model.vehiculoCortesia}}Vehículo cortesía{{/paper-switch}}
{{/if}}

{{paper-divider}}

{{select-job model=listJob selectJob="selectJob" unselectJob="unselectJob"}}

{{#paper-subheader class="md-no-sticky"}}Ó facilite una breve descripción:{{/paper-subheader}}
{{paper-input label='Descripción del problema' value=model.descripcion icon="build" icon-class="action"}}

{{paper-divider}}

{{select-hora model=listHora}}

{{paper-divider}}
{{#paper-button action='saveSolicitud' raised=true primary=true disabled=savingDisabled}}Enviar solicitud{{/paper-button}}
