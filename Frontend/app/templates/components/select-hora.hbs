<table role="grid" tabindex="0" class="md-calendar">
    <tbody role="rowgroup" class="md-calendar-month " md-item-size="265">
        {{#each model as |minuto|}}
        <tr>
            {{#each minuto.listDay as |celda|}}
            <td>
                {{#if celda.minuteHeader}}                
                    {{#select-header  param=celda action="toggleMinuteHeader"}}
                        {{celda.horaShow}}
                    {{/select-header}}
                {{else}}
                    {{#if celda.dayHeader}}
                        {{#select-header  param=celda action="toggleDayHeader"}}
                            {{celda.dayShow}}
                        {{/select-header}}                
                    {{else}}
                        {{icon-hora iconOff="access-time" iconOn="check-circle" checked=celda.checked param=celda action="toggleHora"}}
                    {{/if}}
                {{/if}}
            </td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>


